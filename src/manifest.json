{
  "name": "Injectable Scripts",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Inject my scripts!",
  "homepage_url": "http://localhost",
  "background": {
    "service_worker": "sw.js",
    "type": "module"
  },
  "permissions": ["tabs", "bookmarks", "scripting", "activeTab"],
  "host_permissions": ["*://*/*"]
}
